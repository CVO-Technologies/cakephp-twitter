{
  "name": "cvo-technologies/twitter",
  "type": "cakephp-plugin",
  "description": "Twitter plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Marlin Cremers",
      "email": "m.cremers@cvo-technologies.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "CvoTechnologies\\Twitter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CvoTechnologies\\Twitter\\Test\\": "tests",
      "Cake\\Test\\Fixture\\": "./vendor/cakephp/cakephp/tests/Fixture"
    }
  },
  "require": {
    "cakephp/cakephp": "3.*",
    "muffin/webservice": "dev-feature-orm",
    "fennb/phirehose": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/CVO-Technologies/cakephp-webservice.git"
    }
  ]
}
